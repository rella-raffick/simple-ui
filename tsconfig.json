{
  "compilerOptions": {
    "types": [
      "node",
      "sequelize",
      "express"
    ],
    "target": "es2017",
    "module": "commonjs",
    "lib": [
      "es2017",
      "dom"
    ],
    "declaration": true,
    "sourceMap": true,
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  },
"include": [
  "src/**/*.ts"
],
"exclude": [
  "node_modules"
]
}